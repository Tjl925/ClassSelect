<button wx:if="{{a}}" bindtap="{{c}}" class="action-button"><image class="modify-icon" src="{{b}}" alt="修改"/><text class="button-text">修改</text></button><button wx:else bindtap="{{e}}" class="action-button"><image class="modify-icon" src="{{d}}" alt="举报"/><text class="button-text">举报</text></button><view class="info-container"><view class="course-title">{{f}}</view><view class="attributes"><text class="info">教师：<text class="variant">{{g}}</text></text><text class="info">学分：<text class="variant">{{h}}</text></text><text class="info">评分人数：<text class="variant">{{i}}</text></text><text class="info">开课学院：<text class="variant">{{j}}</text></text><text class="info">开课校区：<text class="variant">{{k}}</text></text><text class="info">课程号：<text class="variant">{{l}}</text></text></view><view class="rating-container"><text class="rating-text">{{m}}</text><view class="stars1"><image wx:for="{{n}}" wx:for-item="star" wx:key="a" src="{{star.b}}" class="star-image"></image></view></view></view><view wx:if="{{o}}" class="loading-container"><text>评论加载中...</text></view><scroll-view class="comments-container" scroll-y="true"><block wx:for="{{p}}" wx:for-item="comment" wx:key="f"><view class="comment-item" bindlongpress="{{comment.e}}"><image class="avatar" src="{{comment.a}}" alt="用户头像"/><view class="comment-info"><view class="header"><text class="nickname">{{comment.b}}</text><text class="created-at">{{comment.c}}</text></view><view class="comment-text">{{comment.d}}</view></view></view></block></scroll-view><view class="dynamic-rating"><text style="font-size:20px">评分：</text><view class="rating"><view wx:for="{{q}}" wx:for-item="n" wx:key="a" class="{{['star', n.b && 'active']}}" bindtap="{{n.c}}"> ★ </view></view></view><view class="footer"><button bindtap="{{r}}" style="font-size:20px;font-weight:bold">创建课程</button><button bindtap="{{s}}" style="font-size:20px;font-weight:bold">评论</button></view>